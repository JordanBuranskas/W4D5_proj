  def sub_sum(nums)
    subs = []
    new_array = []
    (0...nums.length).each do |idx|  #O(n^2)
        (idx...nums.length).each do |idx2|
           substring = nums[idx..idx2]
           subs << substring
        end
    end
    subs.each do |sub| #O(n)
        new_array << sub.sum
    end
    new_array.max
end
array = [5, 3, -7]
p sub_sum(array)











