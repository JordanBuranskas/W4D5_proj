#PHASE 1

def my_min(array)

min = array[0] #

    array.each do |ele1|  #0(n^2)
        array.each do |ele2|
            if ele2 < ele1   
                min = ele2
            end
        end
    end

    min
end





#PHASE 2

# def my_min(array)

#     min = array[0]

#     i = 0
#     while i < array.length - 1
#         if array[i] < array[i + 1]
#             min = array[i]
#         end
#         i += 1
#     end

#     min
   
# end

def my_min(array)  #O(n)

    min = array[0]   #O(1)

    (0...array.length-1).each do |i| #0(1n)
        if array[i] < min #O(1)
            min = array[i + 1] #O(1)
        end
    end
    min
end

list = [ 3, 0, 5, 4, -5, 10, 1, 90 ]
p my_min(list)  # =>  -5



